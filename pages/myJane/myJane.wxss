/** container **/
.container{
  background-color:#eee;
  margin-bottom: 40px;
}
.title{
  width:750rpx;
  padding:10px 0;
  display: flex;
  border-bottom: 5px #fff solid;
}
.title text{
  margin-left:10px;
  color:red;
  font-size:12px;
  font-family: "微软雅黑";
  padding-top:3px;
}
.name{
  width:750rpx;
  padding:10px 0;
  text-indent: 20px;
  display: flex;
  border-bottom: 5px #fff solid;
}
.name text{
  font-size:12px;
  color:#333;
  font-family: "微软雅黑";
  padding-top:3px;
}
.name input{
  width:230rpx;
  height: 22px;
  background-color: #ddd;
  font-size:12px;
  padding-left:20px;
  /* border-bottom: 2px #ddd solid; */
  border-radius: 10px;
}
.user-name text{
  margin-left:20px;
}
.user-name input{
  margin-left:20px;
}
.change .ter{
  margin-left:-20px;
}
.change label{
  margin-left:20px;
  font-size:12px;
}
.marry label{
  margin-left:0;
}
.personal-skills .skills{
  width:100px;
}
.personal-skills .skills .certificate{
  font-size:10px;
  display: block;
  color:#999;
}
.personal-skills input{
  width:400rpx;
  height: 45px;
}
.phone text{
  margin-right:20px;
}
.place text{
  margin-right:32px;
}

.tabbar{
  width: 100%;
  height: 50px;
  position: fixed;
  top: 92%;
  display: flex;
  justify-content: space-between;
  z-index: 1;
}
.tabbar view{
  width: 50%;
  padding: 10px;
  text-align: center;
  border: 1px solid orange;
  border-radius: 10px;
  background: black;
  color: white
}